<div class="card">
  <% if practice.video.attached? %>
    <%= video_tag url_for(practice.video), class: :card__img, controls: true %>
  <% end %>
  <div class="card__body">
    <font color="white"><%= link_to practice.player.name, practice_path(practice.id), class: :card__title%></font>
    <p class="card__summary">
      <%= practice.attempt.name %>
    </p>
    <%= link_to "by #{practice.user.nickname}",root_path, class: :card__user %>
  </div>
</div>