<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <%= link_to "#{current_user.nickname}さん！", root_path, class: :greeting__link%>
        <%= "練習動画をまとめておきましょう！" %>
      </div>
    <% end %>  
    <div class="card__wrapper">
      <%= render partial: 'practice', collection: @practices %>
    </div>
  </div>
</main>
